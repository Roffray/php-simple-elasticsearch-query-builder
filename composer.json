{
    "name": "jclaveau/php-simple-elasticsearch-query-builder",
    "description": "A little class to generate ElasticSearch queries in an SQL way",
    "license": "MIT",
    "keywords": ["elasticsearch", "sql", "simple", "query builder"],
    "authors": [
        {
            "name": "Jean Claveau",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4" : {
            "JClaveau\\ElasticSearch\\" : "src"
        },
        "files": [
              "src/functions.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jclaveau/xhprof",
            "no-api": true
        },
        {
            "type": "vcs",
            "url": "https://github.com/jclaveau/phpunit-testlistener-xhprof",
            "no-api": true
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "jclaveau/phpunit-testlistener-xhprof": "dev-master",
        "jclaveau/xhprof": "dev-master",
        "jclaveau/phpunit-profile-asserts": "^1.0"
    },
    "require": {
        "jclaveau/php-visibility-violator": "1.*",
        "php": ">=5.6.0"
    }
}
